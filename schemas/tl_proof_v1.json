{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Oord TL Proof v1",
    "type": "object",
    "properties": {
      "proof_version": {
        "type": "string",
        "const": "1.0"
      },
      "tl_seq": {
        "type": "integer",
        "minimum": 1
      },
      "merkle_root": {
        "type": "string",
        "pattern": "^cid:sha256:[0-9a-f]{64}$"
      },
      "sth_sig": {
        "type": "string"
      },
      "t_log_ms": {
        "type": "integer",
        "minimum": 0
      },
      "signer_key_id": {
        "type": "string"
      }
    },
    "required": [
      "proof_version",
      "tl_seq",
      "merkle_root",
      "sth_sig",
      "t_log_ms"
    ],
    "additionalProperties": false
  }
